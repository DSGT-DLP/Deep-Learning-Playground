{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-control-regex": 0,
    "camelcase": 0,
    "react/no-unescaped-entities": 0,
    "object-shorthand": ["error", "consistent-as-needed"],
    "no-var": ["error"]
  }
}
